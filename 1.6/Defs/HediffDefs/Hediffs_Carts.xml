<Defs>
<HediffDef>
	<defName>VPE_Ragna_Merchant_Pushcart1_SNTL</defName>
	<label>Pushcart</label>
	<description>A pushcart allowing the user to carry more items.</description>
	<hediffClass>HediffWithComps</hediffClass>
	<initialSeverity>1</initialSeverity>
	<minSeverity>0</minSeverity>
	<maxSeverity>1</maxSeverity>
	<defaultLabelColor>(1, 1, 1)</defaultLabelColor>
	<makesSickThought>False</makesSickThought>
	<scenarioCanAdd>false</scenarioCanAdd>
	<isBad>false</isBad>
	<everCurableByItem>false</everCurableByItem>
	<keepOnBodyPartRestoration>True</keepOnBodyPartRestoration>
	<comps>
		<li Class="HediffCompProperties_DisappearsOnDeath"/>
	</comps>
	<stages>
		<li>
			<statFactors>
				<CarryingCapacity>5</CarryingCapacity>
				<VEF_MassCarryCapacity>5</VEF_MassCarryCapacity>
			</statFactors>
		</li>
	</stages>
	<renderNodeProperties>
		<li>
			<workerClass>PawnRenderNodeWorker_AttachmentBody</workerClass>
			<texPaths>
				<li>UI_RagnarokSTNL/Cart/Base/Base</li>
			</texPaths>
			<overrideMeshSize>(2.25, 2.25)</overrideMeshSize>
			<parentTagDef>Body</parentTagDef>
			<rotDrawMode>Fresh, Rotting</rotDrawMode>
			<drawData>
				<scaleOffsetByBodySize>false</scaleOffsetByBodySize>
				<defaultData>
					<layer>0</layer>
				</defaultData>
				<dataNorth>
					<offset>(0, 0.5, -1)</offset>
					<!-- <layer>-999</layer> -->
				</dataNorth>
				<dataSouth>
					<offset>(0, -0.5, 0.5)</offset>
					<!-- <layer>999</layer> -->
				</dataSouth>
				<dataEast>
					<offset>(-0.65, 0.5, -0.2)</offset>
					<!-- <layer>998</layer> -->
